<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.board.boardstudy.mapper.boardMapper">


    <!-- 게시글 목록 조회 -->
    <select id="boardList" resultType="boardRes">
        SELECT id,
               title,
               writer,
               content,
               created_at,
               view_count
        FROM board
    </select>

</mapper>



